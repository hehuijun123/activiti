package com.ssh.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.GenerationType;
import javax.persistence.Id;

/**
 * 运输
 * @author Administrator
 *
 */
@Entity
public class Transport {
	@Id
	@GeneratedValue(strategy=GenerationType.AUTO)
	private long Tid;	//id
	private String Tnum;	//	订单号
	private String Tdate;	//日期
	private String Tcompany;	//运送单位
	private String Tperson;	//送货人
	private String Twork;
	private String Tother;//备注运送物品
	private String Tr;//记录人
	private String sc;
	private String zs;
	private String jt;
	public long getId() {
		return Tid;
	}
	public void setId(long id) {
		this.Tid = id;
	}
	public String getTdate() {
		return Tdate;
	}
	public void setTdate(String tdate) {
		Tdate = tdate;
	}
	public String getTperson() {
		return Tperson;
	}
	public void setTperson(String tperson) {
		Tperson = tperson;
	}
	public String getTcompany() {
		return Tcompany;
	}
	public void setTcompany(String tcompany) {
		Tcompany = tcompany;
	}
	public String getTnum() {
		return Tnum;
	}
	public void setTnum(String tnum) {
		Tnum = tnum;
	}
	public String getTwork() {
		return Twork;
	}
	public void setTwork(String twork) {
		Twork = twork;
	}
	public String getTother() {
		return Tother;
	}
	public void setTother(String tother) {
		Tother = tother;
	}
	public String getSc() {
		return sc;
	}
	public void setSc(String sc) {
		this.sc = sc;
	}
	public String getZs() {
		return zs;
	}
	public void setZs(String zs) {
		this.zs = zs;
	}
	public String getJt() {
		return jt;
	}
	public void setJt(String jt) {
		this.jt = jt;
	}
	public String getTr() {
		return Tr;
	}
	public void setTr(String tr) {
		Tr = tr;
	}
	
}
